    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "jiti": "^2.5.1",
    "npm": "^11.5.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  }
