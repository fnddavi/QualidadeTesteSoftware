{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "redis-start": "docker run --name redis -p 6379:6379 -d redis:alpine redis-server --requirepass 123",
    "build": "tsc",
    "lint": "eslint .",
    "start": "ts-node ./src",
    "dev": "ts-node-dev ./src",
    "test": "npm run test:up && cross-env NODE_ENV=test jest --coverage --detectOpenHandles --forceExit && npm run test:stop",
    "test:up": "docker compose -f docker-compose.test.yml up -d postgres redis",
    "test:stop": "docker compose -f docker-compose.test.yml stop",
    "test:ci": "cross-env NODE_ENV=ci.containers jest --coverage --detectOpenHandles --forceExit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.14.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.1",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.11.13",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.34.0",
